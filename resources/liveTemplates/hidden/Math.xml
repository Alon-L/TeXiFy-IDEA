<templateSet group="LaTeX">
    <template id="MATH.frac" name="frac" value="{$NUMERATOR$}{$END$}" description="Fraction"
              toReformat="false"
              toShortenFQNames="false">
        <variable name="NUMERATOR" expression="complete()" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="LATEX_MATH" value="true"/>
        </context>
    </template>
</templateSet>
